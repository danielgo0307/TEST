  }

  function financial() {
    switch ($('#financialSelect :selected').val().toString()) {
      case '1':
        $('#optionPay').css("display", "none");
        $('#optionChat').css("display", "none");
        break;
      case '2':
        $('#optionPay').css("display", "block");
        $('#optionChat').css("display", "none");
        break;
      case '3':
        $('#optionPay').css("display", "none");
        $('#optionChat').css("display", "block");
        break;
    }

    $('#btnSend').css("display", "initial");
  }

  $(function () {
    $('[data-toggle="popover"]').popover({html:true}) // Funcion general, html:true identifica etiquetas HTML, para img y texto
  })

  $('.invalid-feedback-login').hide();
  $("#btnIngresar").click(function(){
    if ($('#inputUser').val() != '' && $('#inputPassword').val() != '' ) {
      $('#login').css("display", "none");
      $('#on-logged').css("display", "block");
    } else {
      $('.invalid-feedback-login').show('fast');
    }
  });
</script>
