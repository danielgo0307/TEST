  function nextProduct() {
    const index = products.findIndex(item => {
      return '$' + item.miniumValue.toString() === $('#currentDebt').text()
    });
    const obj = products[(index + 1) === products.length ? 0 :  (index + 1)];
    $('#currentDebtText').text('Tu deuda actual ' + (obj.number.length === 16 ? 'de la ' : 'del ')
      + obj.label + ' terminad' + (obj.number.length === 16 ? 'a' : 'o') + ' en ' + obj.number.substring(obj.number.length - 4) + ' es:');
    $('#currentDebt').text('$'+ obj.miniumValue);
    $('#btnSend').css("display", "none");
    $('#financialSelect').val('0');
    $('#optionPay').css("display", "none");
    $('#optionChat').css("display", "none");
  }

  function maskedNumber(value) {
    let lastFourDigits = value.substring(value.length - 4);
    let groupSize = value.length % 3 === 0 ? 3 : 4;
    let maskedStr = '';
    for (let i = 0; i < value.length - 4; i += groupSize) {
      maskedStr += 'XXXX ';
    }
    return maskedStr += lastFourDigits;
  }

  function update() {
    const obj = products.find(item => {
      return item.id.toString() === $('#inputState :selected').val()
    });
    $('#number').text(maskedNumber(obj.number));
    $('#image').attr("src", obj.image);
    $('#label').text(obj.label);
    $('#productTitle').text('Resumen ' + obj.label);
    $('#cutoffDate').text(obj.cutoffDate);
    $('#paymentDate').text(obj.paymentDate);
    $('#miniumValue').text('$'+ obj.miniumValue);
    $('#miniumValue2').text('$'+ obj.miniumValue);
    $('#currentDebtText').text('Tu deuda actual ' + (obj.number.length === 16 ? 'de la ' : 'del ')
      + obj.label + ' terminad' + (obj.number.length === 16 ? 'a' : 'o') + ' en ' + obj.number.substring(obj.number.length - 4) + ' es:');
    $('#currentDebt').text('$'+ obj.miniumValue);
    $('#totalValue').text('$' + obj.totalValue);
    $('#availableValue').text('$' + obj.availableValue);
    $('#totalValueCard').text('$' + obj.totalValue);
    $('#moreInfo').css("display", "block");
    $('#accordionExample').css("display", "block");
    $("#myChartIn").remove();

    /*$('#canvas-container').append($('<canvas/>',{
      'class':'radHuh',
